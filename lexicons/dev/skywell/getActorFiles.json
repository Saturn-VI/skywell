{
  "lexicon": 1,
  "id": "dev.skywell.getActorProfile",
  "defs": {
    "main": {
      "type": "query",
      "description": "Gets files created by an actor. Paginated.",
      "parameters": {
        "type": "params",
        "required": ["actor"],
        "properties": {
          "actor": {
            "type": "string",
            "format": "at-identifier",
            "description": "Handle or DID of account to get files from."
          },
          "limit": {
            "type": "integer",
            "minimum": 1,
            "maximum": 100,
            "default": 50
          },
          "cursor": {
            "type": "string"
          }
        }
      },
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": ["files"],
          "properties": {
            "cursor": {
              "type": "string"
            },
            "profiles": {
              "type": "array",
              "items": {
                "type": "ref",
                "ref": "dev.skywell.defs#fileView"
              }
            }
          }
        }
      }
    }
  }
}
